{
    "name": "regimenDataSetbase",
    "version": "1.0",
    "tag": "",
    "description": "",
    "uses": [],
    "sources": [
        {
            "table": "etl.hiv_monthly_report_dataset",
            "alias": "hmsd"
        }
    ],
    "columns": [{
        "type": "simple_column",
        "alias": "gender",
        "column": "hmsd.gender"
    }, {
        "type": "simple_column",
        "alias": "arv_first_regimen_location_id",
        "column": "hmsd.arv_first_regimen_location_id"
    }, {
        "type": "derived_column",
        "alias": "age_range",
        "expressionType": "case_statement",
        "expressionOptions": {
            "caseOptions": [{
                "condition": "hmsd.age < 15",
                "value": "lt_15"
            }, {
                "condition": "hmsd.age >= 15",
                "value": "gte_15"
            }]
        }
    }],
    "filters": {
        "conditionJoinOperator": "and",
        "conditions": [
            {
                "filterType": "tableColumns",
                "conditionExpession": "endDate = ?",
                "parameterName": "endDate"
            }
        ]
    }
}